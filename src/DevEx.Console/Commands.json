{
    "Commands": [
        {
            "Name": "vault",
            "Description": "Vault operations",
            "SubCommands": [
                {
                    "Name": "create",
                    "Description": "Create a new entry",
                    "Handler": "DevEx.Modules.Vault.Windows.Handlers.WindowsVaultCreateCommandHandler",
                    "Parameters": [
                        {
                            "Name": "key",
                            "Description": "The key for the entry"
                        },
                        {
                            "Name": "value",
                            "Description": "The value to store"
                        }
                    ]
                },
                {
                    "Name": "read",
                    "Description": "Read an entry",
                    "Handler": "DevEx.Modules.Vault.Windows.Handlers.WindowsVaultReadCommandHandler",
                    "Parameters": [
                        {
                            "Name": "key",
                            "Description": "The key to retrieve"
                        }
                    ]
                },
                {
                    "Name": "update",
                    "Description": "Update an existing entry",
                    "Handler": "DevEx.Modules.Vault.Windows.Handlers.WindowsVaultUpdateCommandHandler",
                    "Parameters": [
                        {
                            "Name": "key",
                            "Description": "The key to update"
                        },
                        {
                            "Name": "value",
                            "Description": "The new value"
                        }
                    ]
                },
                {
                    "Name": "delete",
                    "Description": "Delete an entry",
                    "Handler": "DevEx.Modules.Vault.Windows.Handlers.WindowsVaultDeleteCommandHandler",
                    "Parameters": [
                        {
                            "Name": "key",
                            "Description": "The key to delete"
                        }
                    ]
                }
            ]
        },
        {
            "Name": "run",
            "Description": "Run operations",
            "SubCommands": [
                {
                    "Name": "docker-compose",
                    "Description": "Run Docker Compose Configuration",
                    "Handler": "DevEx.Modules.Run.Docker.DockerComposeRunCommandHandler",
                    "Parameters": []
                }
            ]
        },
        {
            "Name": "intellisense",
            "Description": "Configures CLI IntelliSense on PowerShell",
            "SubCommands": [
                {
                    "Name": "clean",
                    "Description": "Reset IntelliSense Commands",
                    "Handler": "DevEx.Modules.IntelliSense.CleanHandler",
                    "Parameters": []
                }
            ]
        }
    ]
}